{
	"name": "hex-rgb-js",
	"version": "5.0.0",
	"description": "Convert HEX color to RGBA",
	"module": "./index.js",
	"types": "./index.d.ts",
	"main": "./index.cjs",
	"license": "MIT",
	"repository": "sindresorhus/hex-rgb",
	"funding": "https://github.com/sponsors/sindresorhus",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "https://sindresorhus.com"
	},
	"type": "module",
	"exports": {
		".": {
			"import": "./index.js",
			"require": "./index.cjs"
		}
	},
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"test": "xo && ava && tsd",
