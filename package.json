		"colour",
		"convert",
		"conversion",
		"converter",
		"css"
	],
	"devDependencies": {
		"ava": "^3.15.0",
		"rollup": "^2.58.0",
		"tsd": "^0.14.0",
		"xo": "^0.39.1"
	}
